{
  "name": "@lps/volcano",
  "version": "0.0.6",
  "exports": "./src/mod.ts",
  "publish": {
    "include": ["LICENSE", "README.md", "src/**/*.ts"]
  },
  "license": "MIT NON-AI",
  "lock": false,
  "tasks": {
    "test": "deno test -A --unstable-kv",
    "docs": "deno doc --json ./src/mod.ts > denodoc/tree.json"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "exclude": ["**/_fresh/*"],
  "imports": {
    "@cross/image": "jsr:@cross/image@^0.4.1",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/crypto": "jsr:@std/crypto@^1.0.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@std/yaml": "jsr:@std/yaml@^1.0.10",
    "@zod/zod": "jsr:@zod/zod@^4.1.12",
    "slugify": "npm:@sindresorhus/slugify@^3.0.0",
    "unified": "npm:unified@^11.0.5",
    "remark-parse": "npm:remark-parse@^11.0.0",
    "remark-frontmatter": "npm:remark-frontmatter@^5.0.0",
    "remark-directive": "npm:remark-directive@^3.0.0",
    "unist-util-visit": "npm:unist-util-visit@^5.0.0",
    "prism-esm": "npm:prism-esm@^1.29.0-fix.6"
  },
  "nodeModulesDir": "auto"
}
